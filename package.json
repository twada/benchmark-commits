{
  "name": "@twada/benchmark-commits",
  "description": "Run benchmark on specified git commits",
  "version": "0.0.1",
  "author": {
    "name": "Takuto Wada",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "benchmark": "^2.1.4",
    "extract-git-treeish": "^1.0.0"
  },
  "devDependencies": {
    "@twada/mocha-spec-tree-reporter": "^0.2.0",
    "mocha": "^9.0.0",
    "semistandard": "^16.0.0",
    "snazzy": "^9.0.0"
  },
  "scripts": {
    "lint": "semistandard --verbose index.js suite-setup.js test/*test.js | snazzy",
    "fmt": "semistandard --fix index.js suite-setup.js test/*test.js",
    "test": "npm run lint && npm run test:all",
    "test:unit": "mocha --timeout 0 test/suite-setup-test.js",
    "test:doc": "mocha --timeout 0 --reporter @twada/mocha-spec-tree-reporter test",
    "test:all": "mocha --timeout 0 test"
  },
  "semistandard": {
    "globals": [
      "describe",
      "context",
      "beforeEach",
      "afterEach",
      "it"
    ]
  }
}
